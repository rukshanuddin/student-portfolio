<h1><a href="/projects/<%=@project.id%>"><%= @project.title %></a></h1>
<p>A project by <%=@project.user.firstname%> <%=@project.user.lastname%></p>
<p>a.k.a. <a href="http://github.com/<%=@project.user.username%>"><b><%=@project.user.username%></b></a></p>

<h3><%= @project.module%></h3>
<br>
<p><%= @project.description %></p>
<p><%= @project.github%></p>
<% if @project.user.id == current_user.id%>
<form method="post" action="/projects/<%=@project.id%>/delete">
  <input id="hidden" type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete Project">
</form>
<br>
<a href="/projects/<%=@project.id%>/edit">Edit Project </a>
<%end%>